{
  "name": "@chubbyts/chubbyts-packaging",
  "type": "module",
  "version": "2.0.6",
  "description": "Packaging helper",
  "keywords": [
    "chubbyts",
    "packaging",
    "helper"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-packaging",
  "scripts": {
    "cs-fix": "prettier --write build.js",
    "cs": "prettier --check build.js",
    "lint-fix": "eslint build.js --fix",
    "lint": "eslint build.js"
  },
  "prettier": {
    "printWidth": 120,
    "tabWidth": 2,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "files": [
    "build.js",
    "tsconfig.cjs.json",
    "tsconfig.esm.json",
    "tsconfig.json",
    "tsconfig.types.json"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@chubbyts/chubbyts-eslint": "^2.0.2",
    "prettier": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
