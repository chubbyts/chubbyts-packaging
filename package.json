{
  "name": "@chubbyts/chubbyts-packaging",
  "type": "module",
  "version": "1.1.1",
  "description": "Packaging helper",
  "keywords": [
    "chubbyts",
    "packaging",
    "helper"
  ],
  "author": "Dominik Zogg",
  "license": "MIT",
  "repository": "chubbyts/chubbyts-packaging",
  "scripts": {
    "cs-fix": "prettier --write rename-commonjs.js",
    "cs": "prettier --check rename-commonjs.js",
